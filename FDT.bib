@Article{CV,
	author="Carr, Robert
	and Vempala, Santosh",
	title="On the Held-Karp relaxation for the asymmetric and symmetric traveling salesman problems",
	journal="Mathematical Programming",
	year="2004",
	month="Jul",
	day="01",
	volume="100",
	number="3",
	pages="569--587",
	abstract="A long-standing conjecture in combinatorial optimization says that the integrality gap of the famous Held-Karp relaxation of the metric STSP (Symmetric Traveling Salesman Problem) is precisely 4/3. In this paper, we show that a slight strengthening of this conjecture implies a tight 4/3 integrality gap for a linear programming relaxation of the metric ATSP (Asymmetric Traveling Salesman Problem). Our main tools are a new characterization of the integrality gap for linear objective functions over polyhedra, and the isolation of ``hard-to-round'' solutions of the relaxations.",
	issn="1436-4646",
	doi="10.1007/s10107-004-0506-y",
	url="https://doi.org/10.1007/s10107-004-0506-y"
}


