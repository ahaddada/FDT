@Article{goemans,
	author="Goemans, M. X.
	and Ramakrishnan, V. S.",
	title="Minimizing submodular functions over families of sets",
	journal="Combinatorica",
	year="1995",
	month="Dec",
	day="01",
	volume="15",
	number="4",
	pages="499--513",
	abstract="We consider the problem of characterizing the minimum of a submodular function when the minimization is restricted to a family of subsets. We show that, for many interesting cases, there exist two elementsa andb of the groundset such that the problem is equivalent to the problem of minimizing the submodular function over the sets containinga but notb. This leads to a polynomial-time algorithm for minimizing a submodular function over these families of sets. Our results apply, for example, to the families of odd cardinality subsets or even cardinality subsets separating two given vertices, or to the complement of a lattice family of subsets. We also derive that the second smallest value of a submodular function over a lattice family can be computed in polynomial-time. These results generalize and unify several known results.",
	issn="1439-6912",
	doi="10.1007/BF01192523",
	url="https://doi.org/10.1007/BF01192523"
}



