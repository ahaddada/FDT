@Article{Karmarkar1984,
	author="Karmarkar, N.",
	title="A new polynomial-time algorithm for linear programming",
	journal="Combinatorica",
	year="1984",
	month="Dec",
	day="01",
	volume="4",
	number="4",
	pages="373--395",
	abstract="We present a new polynomial-time algorithm for linear programming. In the worst case, the algorithm requiresO(n3.5L) arithmetic operations onO(L) bit numbers, wheren is the number of variables andL is the number of bits in the input. The running-time of this algorithm is better than the ellipsoid algorithm by a factor ofO(n2.5). We prove that given a polytopeP and a strictly interior point a $\epsilon$P, there is a projective transformation of the space that mapsP, a toP{\textasciiacutex}, a{\textasciiacutex} having the following property. The ratio of the radius of the smallest sphere with center a{\textasciiacutex}, containingP{\textasciiacutex} to the radius of the largest sphere with center a{\textasciiacutex} contained inP{\textasciiacutex} isO(n). The algorithm consists of repeated application of such projective transformations each followed by optimization over an inscribed sphere to create a sequence of points which converges to the optimal solution in polynomial time.",
	issn="1439-6912",
	doi="10.1007/BF02579150",
	url="https://doi.org/10.1007/BF02579150"
}

@Article{HanafiT2017,
	author="Hanafi,Sa{\"{i}}d and Todosijevi{\'{c}}, Raca",
	title="Mathematical programming based heuristics for the 0-1 MIP: a survey",
	journal="Journal of Heuristics",
	year="2017",
	month="Aug",
	volume="23",
	number="4",
	pages="165-206",
	abstract="The 0–1 mixed integer programming problem is used for modeling many combinatorial problems, ranging from logical design to scheduling and routing as well as encompassing graph theory models for resource allocation and financial planning. This paper provides a survey of heuristics based on mathematical programming for solving 0–1 mixed integer programs (MIP). More precisely, we focus on the stand-alone heuristics for 0–1 MIP as well as those heuristics that use linear programming techniques or solve a series of linear programming models or reduced problems, deduced from the initial one, in order to produce a high quality solution of a considered problem. Our emphasis will be on how mathematical programming techniques can be used for approximate problem solving, rather than on comparing performances of heuristics.",
	doi="10.1007/s10732-017-9336-y",
}


